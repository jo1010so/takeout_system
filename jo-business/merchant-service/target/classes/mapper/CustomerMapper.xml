<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jojo.mapper.CustomerMapper">

    <resultMap id="BaseResultMap" type="com.jojo.domain.Customer">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="username" column="username" jdbcType="VARCHAR"/>
            <result property="gender" column="gender" jdbcType="INTEGER"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="phone" column="phone" jdbcType="VARCHAR"/>
            <result property="status" column="status" jdbcType="INTEGER"/>
            <result property="avatar" column="avatar" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="idcard" column="IDCard" jdbcType="INTEGER"/>
            <result property="authorize" column="authorize" jdbcType="INTEGER"/>
            <result property="bucket" column="bucket" jdbcType="VARCHAR"/>
            <result property="object" column="object" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,username,gender,
        password,phone,status,
        avatar,create_time,IDCard,
        authorize,bucket,object
    </sql>
</mapper>
